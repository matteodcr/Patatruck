Voiture(Drive) {
    * (Drive) 
    | Cell(_,@) ? Pop :(Stop) 
    | Cell(F,O) & Cell(R,J) ? Wizz(R) :(Roule)                                          
    | Cell(F,O) & Cell(L,J) ? Wizz(L) :(Roule)
    | Cell(H,G) ? 10%Wizz(R) / 10%Wizz(L) / Wizz(F) :(Roule) 
    | True ? Wizz(F) :(Roule) 

    * (Stop) 
    | Cell(_,@) ? :(Stop)  
    | True ? Wizz :(Roule) 
}
