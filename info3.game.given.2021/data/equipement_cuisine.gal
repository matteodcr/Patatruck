Firecook(Init){
    *(Init)
    |Key(SPACE) & Cell(F,@) ? Pop:(Cook)         
    
    *(Cook)
    |True ?     Wait:(Cooked)  
    		
    *(Cooked)
    |Key(SPACE) & Cell(F,@) ? Wizz:(Presswait)  
    |True ? Explode:(Init)
    
    *(Presswait)
    |(!Key(SPACE)) & Cell(F,@) ? :(Init)  
}

